{
	"name": "finsight-ai",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"fdev": "vite --open",
		"dev": "wrangler dev",
		"build": "tsc -b && vite build",
		"lint": "eslint .",
		"preview": "npm run build && vite preview",
		"deploy": "npm run build && wrangler deploy",
		"db:migration:generate": "drizzle-kit generate",
		"db:migration:apply:local": "wrangler d1 migrations apply finsight-ai-db --local",
		"db:migration:apply:remote": "wrangler d1 migrations apply finsight-ai-db --remote",
		"db:execute:local": "wrangler d1 execute finsight-ai-db --local --command"
	},
	"devDependencies": {
		"@cloudflare/vitest-pool-workers": "^0.8.19",
		"@cloudflare/workers-types": "^4.20251126.0",
		"@types/node": "^24.10.1",
		"@types/react": "^19.2.7",
		"@types/react-dom": "^19.2.3",
		"typescript": "^5.5.2",
		"vitest": "~3.2.0",
		"wrangler": "^4.50.0"
	},
	"dependencies": {
		"@tsndr/cloudflare-worker-jwt": "^3.2.0",
		"@vitejs/plugin-react-swc": "^4.2.2",
		"bcryptjs": "^3.0.3",
		"chanfana": "^2.8.3",
		"drizzle-kit": "^0.31.7",
		"drizzle-orm": "^0.44.7",
		"itty-router": "^5.0.22",
		"path": "^0.12.7",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"react-turnstile": "^1.1.4",
		"url": "^0.11.4"
	}
}
